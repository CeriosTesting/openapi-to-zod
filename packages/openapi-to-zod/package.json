{
	"name": "@cerios/openapi-to-zod",
	"version": "1.5.0",
	"description": "Generate Zod schemas from OpenAPI specifications. A TypeScript code generator that converts OpenAPI/Swagger YAML definitions into type-safe Zod validation schemas.",
	"keywords": [
		"api",
		"cerios",
		"codegen",
		"generator",
		"openapi",
		"schema",
		"typescript",
		"validation",
		"yaml",
		"zod"
	],
	"homepage": "https://github.com/CeriosTesting/openapi-codegen#readme",
	"bugs": {
		"url": "https://github.com/CeriosTesting/openapi-codegen/issues"
	},
	"license": "MIT",
	"author": "Ronald Veth - Cerios",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CeriosTesting/openapi-codegen.git"
	},
	"bin": {
		"openapi-to-zod": "./dist/cli.js"
	},
	"files": [
		"dist"
	],
	"type": "commonjs",
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"scripts": {
		"build": "tsup",
		"changeset": "pnpm changeset",
		"check-exports": "attw --pack .",
		"compile": "tsc --noEmit",
		"dev": "tsup --watch",
		"pack": "pnpm pack --pack-destination=../..",
		"test": "vitest run",
		"test:generate": "node dist/cli.js -i src/phx-case-management-api.openapi.yaml -o output/schemas.ts"
	},
	"dependencies": {
		"@cerios/openapi-core": "workspace:*",
		"@cerios/openapi-to-typescript": "workspace:*",
		"commander": "catalog:",
		"cosmiconfig": "catalog:",
		"minimatch": "catalog:",
		"yaml": "catalog:"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "catalog:",
		"@changesets/cli": "catalog:",
		"@types/node": "catalog:",
		"@types/prompts": "catalog:",
		"husky": "catalog:",
		"lint-staged": "catalog:",
		"prompts": "catalog:",
		"tsup": "catalog:",
		"tsx": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:",
		"zod": "catalog:"
	},
	"peerDependencies": {
		"zod": "^4.0.0"
	}
}
